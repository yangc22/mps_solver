# Sage, A. P. and White, C. C., Optimum Systems Control,
# Prentice-Hall, 1977.

StateVariables 			= [x1, x2];
ControlVariables 		= [u1];
InitialConstraints 		= [x1 - 1.0, x2 - 1.0];
TerminalConstraints 	= [x1, x2];
CostFunctional 			= u1*u1*0.5;
#TerminalPenalty         = x1*x1;
DifferentialEquations 	= [x2, u1];
Nodes					= 101;
Tolerance				= 1.0e-6;
OutputFile				= "ex2.data";
Display					= YES;
